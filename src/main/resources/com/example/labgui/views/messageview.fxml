        <TextField fx:id="messageField" layoutX="30" layoutY="338" prefHeight="26" prefWidth="361" />
        <ScrollPane fx:id="scrollMain" fitToWidth="true" layoutX="30" layoutY="70" prefHeight="256" prefWidth="418">
            <content>
                <VBox fx:id="vboxMessages" prefHeight="248" prefWidth="412" />
            </content>
        </ScrollPane>
    </children>

</AnchorPane>
