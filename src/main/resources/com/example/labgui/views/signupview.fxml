<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.labgui.controller.SignupController">
   <children>
      <PasswordField fx:id="signupPasswordField" layoutX="226.0" layoutY="243.0" promptText="password" />
      <Text layoutX="243.0" layoutY="110.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Sign up">
         <font>
            <Font size="34.0" />
         </font>
      </Text>
      <TextField fx:id="signupUsernameField" layoutX="226.0" layoutY="160.0" promptText="username" />
      <TextField fx:id="signupEmailField" layoutX="227.0" layoutY="200.0" promptText="email" />
      <Button fx:id="buttonSignup" layoutX="226.0" layoutY="286.0" mnemonicParsing="false" onAction="#handleButtonSignup" prefHeight="26.0" prefWidth="150.0" text="Sign up" />
      <Text layoutX="226.0" layoutY="349.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Already have an account?">
         <font>
            <Font size="10.0" />
         </font>
      </Text>
      <Hyperlink fx:id="loginHyperlink" layoutX="337.0" layoutY="333.0" onAction="#handleLoginHyperlink" text="Log in" />
   </children>
</AnchorPane>
